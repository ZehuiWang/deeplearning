public static double mean(double[] m){
  double sum = 0;
  for(int i = 0;i < m.length;i++){
    sum += m[i];
  }
  return sum/m.length;
}


public static double median (double[] m){
  Arrays.sort(nums);
  double median;
  if(nums.length % 2 == 0){
    median = (double)((nums[nums.length/2] + nums[nums.length/2 -1 ]))/2;
  }else{
     median = (double)(nums[nums.length/2]);
   }
   return median;
}
